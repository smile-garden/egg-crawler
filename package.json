{
  "name": "egg-crawler",
  "version": "1.0.0",
  "description": "A crawler tool",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^3",
    "egg-scripts": "^2",
    "egg-view-nunjucks": "^2.3.0",
    "exceljs": "^4.3.0",
    "qs": "^6.11.2"
  },
  "devDependencies": {
    "egg-bin": "^5",
    "egg-ci": "^2",
    "egg-mock": "^5",
    "eslint": "^8",
    "eslint-config-egg": "^12"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --port=7008 --daemon --title=egg-server-egg-reptile",
    "stop": "egg-scripts stop --title=egg-server-egg-reptile",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov"
  },
  "ci": {
    "version": "16, 18",
    "type": "github"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "one smile",
  "license": "MIT"
}
